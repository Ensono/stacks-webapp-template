{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "Launch test files with TestCafe",
      "program": "${workspaceFolder}/packages/template-cli/templates/src/ssr/node_modules/.bin/testcafe",
      "args": ["__tests__/testcafe/fixtures/get-menu.test.cf.ts"],
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": false,
      "outputCapture": "std",
      "cwd": "${workspaceFolder}/packages/template-cli/templates/src/ssr"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "template-cli",
      "program": "${workspaceFolder}/packages/template-cli/dist/index.js",
      "cwd": "${workspaceFolder}/packages/template-cli",
      "protocol": "inspector",
      "outFiles": ["${workspaceFolder}/packages/template-cli/dist/*/.js"],
      "console": "internalConsole",
      "preLaunchTask": "RunBuildCli",
      "outputCapture": "std",
      "disableOptimisticBPs": false,
      "env": {
        "NODE_ENV": "dev",
        "LOG_LEVEL": "debug",
        "COMMAND_TO_RUN": "ininTemplateCli"
      },
      "args": ["./src/__tests__/domain/test.bootstrap-config.json"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "vscode-jest-tests",
      "program": "${workspaceFolder}/packages/template-cli/node_modules/.bin/jest",
      "protocol": "inspector",
      "cwd": "${workspaceFolder}/packages/template-cli",
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": false,
      "outputCapture": "std",
      "args": [
        "--runInBand",
        "--config jest.config.json",
        // "--coverage",
        "--env=node",
        // "--ci",
        "--colors",
        "${workspaceFolder}/packages/template-cli/src/__tests__/domain/prompt.test.ts"
      ],
      "env": {
        "NODE_ENV": "dev"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "pact-test-jest",
      "program": "${workspaceFolder}/packages/template-cli/templates/src/ssr/node_modules/.bin/jest",
      "protocol": "inspector",
      "cwd": "${workspaceFolder}/packages/template-cli/templates/src/ssr",
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": false,
      "outputCapture": "std",
      "args": [
        "--runInBand",
        "--config jest-pact.config.json",
        "--rootDir=__tests__",
        "--env=node",
        "--colors",
        "${workspaceFolder}/packages/template-cli/templates/src/ssr/__tests__/get-menu-by-id.test.pact.ts"
      ],
      "env": {
        "CI": "true",
        "NODE_ENV": "dev",
        "PACT_CONSUMER": "GenericMenuConsumer",
        "PACT_PROVIDER": "MenuApi",
        "PACT_BROKER": "https://amido-stacks.pact.dius.com.au",
        "PACT_BEARER_TOKEN": "FILL_ME_IN"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "templated-web-app",
      "program": "${workspaceFolder}/packages/template-cli/templates/src/ssr/dist/server/index.js",
      "cwd": "${workspaceFolder}/packages/template-cli/templates/src/ssr",
      "protocol": "inspector",
      "sourceMaps": true,
      "console": "internalConsole",
      "outputCapture": "std",
      "preLaunchTask": "RunBuildTemplatedApp",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "debug",
        "PORT": "3000",
        "CI": "true",
        "APPINSIGHTS_INSTRUMENTATIONKEY": "",
        "APP_BASE_PATH": "",
        "APP_BASE_URL": "http://localhost",
        "MENU_API_URL": "http://dev.amidostacks.com/api/menu",
        "PACT_BROKER": "https://amido-stacks.pact.dius.com.au"
      },
      "args": [
        // "--project tsconfig.server.json"
      ]
    }
  ]
}
