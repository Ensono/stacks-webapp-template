  "private": true,
  "scripts": {
    "test": "env NEXT_PUBLIC_CONTENTFUL_SPACE_ID=foo env NEXT_PUBLIC_CONTENTFUL_ACCESS_TOKEN=bar env NEXT_PUBLIC_CONTENTFUL_PREVIEW_ACCESS_TOKEN=baz env APP_BASE_PATH=\"\" node_modules/.bin/jest -c jest.config.json --runInBand",
    "test:watch": "node_modules/.bin/jest -c jest.config.json --watchAll --rootDir . --runInBand ",
    "dev": "nodemon",
    "build": "node_modules/.bin/tsc --project tsconfig.server.json && node_modules/.bin/next build",
    "start": "node dist/server/index.js",
    "lint": "node_modules/.bin/eslint . -f codeframe",
    "prettier": "node_modules/.bin/prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "validate": "npm run prettier --list-different && npm run lint",
    "test:pact": "node_modules/.bin/jest --c jest.pact.config.json --runInBand",
    "test:pact-publish": "node_modules/.bin/pact-publish",
    "test:pact-start-stub": "node_modules/.bin/pact-start-stub",
    "test:pact-can-i-deploy-ci": "node_modules/.bin/pact-can-i-deploy",
    "test:pact-can-i-deploy-cli": "node_modules/.bin/pact-broker can-i-deploy --pacticipant $PACT_CONSUMER -l --broker-base-url $PACT_BROKER --broker-token $PACT_BEARER_TOKEN",
    "test:cypress:watch": "node_modules/.bin/tsc --watch --project __tests__/cypress/tsconfig.cypress.json",
    "test:cypress:compile": "node_modules/.bin/tsc --project __tests__/cypress/tsconfig.cypress.json",
    "test:cypress:run": "npm run test:cypress:compile && node_modules/.bin/cypress run --spec \"**/*.cy.js\"",
    "test:cypress:e2e:run": "npm run test:cypress:run -- --spec \"**/*.test.cy.js\"",
    "test:cypress:axe:run": "npm run test:cypress:run -- --spec \"**/*.test.axe.cy.js\"",
    "test:cypress:eyes:run": "npm run test:cypress:run -- --spec \"**/*.test.eyes.cy.js\"",
    "test:cypress:open": "npm run test:cypress:compile && node_modules/.bin/cypress open",
    "test:cypress": "env CI=true node_modules/.bin/start-server-and-test start $APP_BASE_URL:$PORT test:cypress:run",
    "test:cypress:axe": "env CI=true node_modules/.bin/start-server-and-test start $APP_BASE_URL:$PORT test:cypress:axe:run",
    "test:cypress:eyes": "env CI=true node_modules/.bin/start-server-and-test start $APP_BASE_URL:$PORT test:cypress:eyes:run",
    "test:cypress:e2e": "env CI=true node_modules/.bin/start-server-and-test start $APP_BASE_URL:$PORT test:cypress:e2e:run",
    "clean:tree": "git clean -idx"
  },
