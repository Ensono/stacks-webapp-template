{
  "reporterEnabled": "spec, junit, mochawesome",
  "junitReporterOptions": {
    "mochaFile": "cypress-junit-test-report.xml",
    "toConsole": true
  },
  "mochawesomeReporterOptions": {
    "reportDir": "results",
    "overwrite": false,
    "html": false,
    "json": true,
    "timestamp": "yyyymmdd_HHMMss"
  }
}
