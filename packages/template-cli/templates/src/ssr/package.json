{
  "name": "webapp",
  "scripts": {
    "test": "node_modules/.bin/jest -c jest.config.json --rootDir . --runInBand ",
    "test:watch": "node_modules/.bin/jest -c jest.config.json --watchAll --rootDir . --runInBand ",
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "lint": "node_modules/.bin/eslint . -f codeframe",
    "prettier": "node_modules/.bin/prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "validate": "npm run prettier --list-different && npm run lint",
    "test:pact": "node_modules/.bin/jest --c jest-pact.config.json --rootDir __test__ --runInBand ",
    "test:pact-publish": "node_modules/.bin/tsc ./__tests__/pact/pactPublish.ts && node ./__tests__/pact/pactPublish.js",
    "test:pact-start-stub": "node_modules/.bin/tsc ./__tests__/pact/packStubServer.ts && node __tests__/pact/packStubServer.js",
    "test:pact-can-i-deploy": "node_modules/.bin/tsc ./__tests__/pact/pactCanIDeploy.ts && node __tests__/pact/pactCanIDeploy.js",
    "test:pact-can-i-deploy-ci": "node_modules/.bin/pact-broker can-i-deploy --pacticipant $PACT_CONSUMER -l --broker-base-url $PACT_BROKER --broker-token $PACT_BEARER_TOKEN",
    "test:cypress:compile": "node_modules/.bin/tsc --project tsconfig.cypress.json",
    "test:cypress:run": "node_modules/.bin/tsc --project tsconfig.cypress.json && node_modules/.bin/cypress run",
    "test:cypress:open": "node_modules/.bin/tsc --project tsconfig.cypress.json && node_modules/.bin/cypress open",
    "test:cypress": "node_modules/.bin/start-server-and-test start $APP_BASE_URL:$PORT test:cypress:run",
    "clean:tree": "git clean -idx"
  },
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.9.1",
    "@microsoft/applicationinsights-web": "^2.4.4",
    "applicationinsights": "^1.7.2",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "isomorphic-unfetch": "^3.0.0",
    "morgan": "^1.9.1",
    "next": "^9.2",
    "react": "^16.12.0",
    "react-dom": "^16.7.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^13.1.7",
    "typescript": "^3.7.5",
    "@amido-stacks/eslint-config": "^0.7.0",
    "@types/applicationinsights": "^0.20.0",
    "@pact-foundation/pact": "^9.6.1",
    "@pact-foundation/pact-node": "^10.2.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/cypress-axe": "^0.4.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.9.1",
    "@types/jest-axe": "^3.2.1",
    "@types/react": "^16.9.17",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "axe-core": "^3.5.1",
    "cypress": "^4.0.2",
    "cypress-axe": "^0.5.3",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.3.0",
    "jest-junit": "^10.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "snapshot-diff": "^0.7.0",
    "start-server-and-test": "^1.10.8",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.5.4"
  },
  "config": {
    "pact_do_not_track": true
  },
  "license": "MIT",
  "repository": {
    "url": "git://github.com/amido/stacks-webapp-template"
  }
}
