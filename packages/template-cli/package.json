{
  "name": "@amido-stacks/scaffolding-cli",
  "version": "1.0.0",
  "description": "scaffolding-cli creates a scaffolded folder with deploy and build and src folders ready to be committed to a Git repo",
  "main": "dist/index.js",
  "scripts": {
    "test": "node_modules/.bin/jest -c jest.config.json --coverage",
    "test:watch": "node_modules/.bin/jest -c jest.config.json --coverage --watchAll",
    "lint": "node_modules/.bin/eslint . -f codeframe",
    "view:coverage": "open coverage/lcov-report/index.html",
    "build": "tsc",
    "build:watch": "npm run build -- -w",
    "clean:tree": "git clean -idx"
  },
  "author": "Amido Stacks <stacks@amido.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "fs-extra": "^8.1.0",
    "prompts": "^2.3.0",
    "replace-in-file": "^5.0.2",
    "simple-winston-logger-abstraction": "^2.0.0",
    "typescript": "3.7.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.7",
    "@types/prompts": "^2.0.3",
    "@types/jest": "^25.1.1",
    "babel-jest": "^25.1.0",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "ts-jest": "^25"
  }
}
