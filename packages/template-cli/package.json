{
  "name": "@amidostacks/scaffolding-cli",
  "version": "1.12.5",
  "description": "scaffolding-cli creates a scaffolded folder with deploy, build and src folders ready to be committed to a Git repo of your choosing",
  "preferGlobal": true,
  "bin": {
    "scaffolding-cli": "./dist/index.js",
    "amido-scaffolding-cli": "./dist/index.js"
  },
  "files": [
    "dist/index.js",
    "dist/domain/",
    "templates",
    "src/index.ts",
    "src/domain"
  ],
  "scripts": {
    "test": "node_modules/.bin/jest -c jest.config.json --coverage",
    "test:watch": "node_modules/.bin/jest -c jest.config.json --coverage --watchAll",
    "lint": "node_modules/.bin/eslint . -f codeframe",
    "view:coverage": "open coverage/lcov-report/index.html",
    "build": "tsc",
    "build:watch": "npm run build -- -w",
    "clean:tree": "git clean -idx"
  },
  "author": "Amido Stacks <stacks@amido.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@types/chalk": "^2.2.0",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "prompts": "^2.3.1",
    "replace-in-file": "^5.0.2",
    "simple-git": "^1.132.0",
    "simple-winston-logger-abstraction": "^2.0.0",
    "typescript": "3.7.5"
  },
  "devDependencies": {
    "@types/concat-stream": "^1.6.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.7",
    "@types/prompts": "^2.0.3",
    "babel-jest": "^25.1.0",
    "concat-stream": "^2.0.0",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "ts-jest": "^25"
  },
  "homepage": "https://github.com/amido/stacks-webapp-template/tree/master/packages/template-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/amido/stacks-webapp-template.git",
    "directory": "packages/template-cli"
  },
  "gitHead": "15e8fc25b5715fe95ec1b5db73f224894dc5e53d"
}
